<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
    targetNamespace="http://www.reusability" xmlns:re="http://www.reusability">

    <xs:complexType name="Tpodaci_povereniku">
        <xs:sequence>
            <xs:element name="uloga" type="xs:string"/>
            <xs:element name="adesa_za_postu" type="re:Tadresa_za_postu"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Tadresa_za_postu">
        <xs:sequence>
            <xs:element name="mesto" type="xs:string"/>
            <xs:element name="ulica">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="xs:string">
                            <xs:attribute name="broj" type="xs:int"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Tzalilac">
        <xs:sequence>
            <xs:element name="tip_lica" type="re:Ttip_lica"/>
            <xs:element name="adresa" type="re:Tadresa_za_postu"/>
            <xs:element name="sediste_zalioca" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Ttip_lica">
        <xs:choice>
            <xs:element name="osoba" type="re:Tosoba"/>
            <xs:element name="preduzece" type="xs:string"/>
        </xs:choice>
    </xs:complexType>
    <xs:complexType name="Tosoba">
        <xs:sequence>
            <xs:element name="ime" type="xs:string"/>
            <xs:element name="prezime" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Tprotiv_resenja_zakljucka">
        <xs:sequence>
            <xs:element name="naziv_organa_koji_je_doneo_odluku" type="xs:string"/>
            <xs:element name="broj" type="xs:int"/>
            <xs:element name="od_godine" type="xs:date"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType mixed="true" name="Tsadrzaj">
        <xs:sequence>
            <xs:element name="datum" type="xs:date"/>
            <xs:element name="osnova_za_zalbu" type="xs:string"/>
            <xs:element name="clan"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType mixed="true" name="Tclan">
        <xs:sequence>
            <xs:element minOccurs="0" name="stav">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="xs:string">
                            <xs:attribute name="broj" type="xs:int"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Tpodaci_o_podnosiocu">
        <xs:sequence>
            <xs:element name="osoba" type="re:Tosoba"/>
            <xs:element name="adresa" type="re:Tadresa_za_postu"/>
            <xs:element name="drugi_podaci_za_kontakt" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Tnapomena">
        <xs:sequence>
            <xs:element name="naslov" type="xs:string"/>
            <xs:element maxOccurs="unbounded" name="tacka" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType mixed="true" name="Tsadrzaj_zalbe">
        <xs:sequence>
            <xs:element name="clan" type="re:Tclan"/>
            <xs:element name="ciljani_organ_vlasti" type="xs:string"/>
            <xs:element name="razlog_zalbe" type="re:Trazlog_zalbe"/>
            <xs:element name="datum" type="xs:date"/>
            <xs:element name="podaci_o_zahtjevu_i_informacijama" type="xs:string"/>
            <xs:element name="napomena" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Trazlog_zalbe">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" name="opcija">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="xs:string">
                            <xs:attribute name="izabrana" type="xs:string"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Tciljani_organ_vlasti">
        <xs:sequence>
            <xs:element name="nazivOrgana" type="xs:string"/>
            <xs:element name="sedisteOrgana" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType mixed="true" name="Ttekst_zahteva">
        <xs:sequence>
            <xs:element name="clan" type="re:Tclan"/>
            <xs:element name="opcije" type="re:Topcije"/>
            <xs:element name="informacije_o_zahtevu" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Topcije">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" name="opcija" type="xs:string"/>
            <xs:element name="nacini_dostave" type="re:Tnacini_dostave" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Tnacini_dostave">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="1" name="nacin_dostave">
                <xs:complexType mixed="true">
                    <xs:sequence minOccurs="1">
                        <xs:element minOccurs="0" name="nacin_dostave_input" type="xs:string"/>
                    </xs:sequence>
                    <xs:attribute name="izabran" type="xs:boolean"/>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Tinformacije_o_traziocu">
        <xs:sequence>
            <xs:element name="lice" type="re:Ttip_lica"/>
            <xs:element name="adresa" type="re:Tadresa_za_postu"/>
            <xs:element name="drugi_podaci_za_kontakt" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Tfusnote">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" name="fusnota" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Tinformacije_o_podnosiocu">
        <xs:sequence>
            <xs:element name="lice" type="re:Ttip_lica"/>
            <xs:element name="adresa" type="re:Tadresa_za_postu"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType mixed="true" name="Ttekst_zahteva_obavestenja">
        <xs:sequence>
            <xs:element name="clan" type="re:Tclan"/>
            <xs:element name="godina" type="xs:string"/>
            <xs:element name="opis_trazene_informacije" type="xs:string"/>
            <xs:element name="dan" type="xs:string"/>
            <xs:element name="vreme" type="xs:string"/>
            <xs:element name="radno_vreme" type="re:Tradno_vreme"/>
            <xs:element name="adresa" type="re:Tadresa_za_postu"/>
            <xs:element name="broj_kancelarije" type="xs:int"/>
            <xs:element name="opis_troskova" type="re:Topis_troskova"/>
            <xs:element name="ukupan_trosak" type="re:Tukupan_trosak"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType mixed="true" name="Tradno_vreme">
        <xs:sequence>
            <xs:element name="pocetak" type="xs:string"/>
            <xs:element name="kraj" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType mixed="true" name="Topis_troskova">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="1" name="cena" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType mixed="true" name="Tukupan_trosak">
        <xs:sequence>
            <xs:element name="iznos" type="xs:int"/>
            <xs:element name="broj_racuna" type="xs:string"/>
            <xs:element name="poziv_na_broj" type="xs:int"/>
        </xs:sequence>
    </xs:complexType>
</xs:schema>
